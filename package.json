{
  "name": "level-bench",
  "version": "0.0.1",
  "description": "Benchmark abstract-leveldown and levelup stores",
  "license": "MIT",
  "bin": "level-bench.js",
  "main": "level-bench.js",
  "scripts": {
    "test": "standard && hallmark && (nyc -s tape test/*.js | faucet) && nyc report",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "hallmark": "hallmark --fix",
    "dependency-check": "dependency-check --no-dev .",
    "prepublishOnly": "npm run dependency-check"
  },
  "dependencies": {
    "bytes": "^3.1.0",
    "current-commit": "0.0.0",
    "deep-extend": "~0.6.0",
    "du": "^1.0.0",
    "fast-deep-equal": "^2.0.1",
    "fast-glob": "^3.0.4",
    "human-number": "^1.0.3",
    "keyspace": "^0.0.1",
    "level-test": "^7.1.0",
    "mkdirp": "~0.5.1",
    "reachdown": "^1.0.0",
    "resolve": "^1.12.0",
    "subarg": "^1.0.0",
    "tmp-redis": "^1.0.0"
  },
  "devDependencies": {
    "dependency-check": "^4.1.0",
    "faucet": "^0.0.1",
    "hallmark": "^2.0.0",
    "level-community": "^3.0.0",
    "nyc": "^14.1.1",
    "standard": "^14.3.1",
    "tape": "^4.10.1"
  },
  "hallmark": {
    "community": "level-community"
  },
  "repository": "Level/bench",
  "homepage": "https://github.com/Level/bench",
  "keywords": [
    "level",
    "levelup",
    "abstract-leveldown",
    "benchmark"
  ],
  "engines": {
    "node": ">=8"
  }
}
